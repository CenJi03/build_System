[run]
source = .
omit =
    */venv/*
    */env/*
    */migrations/*
    */tests/*
    manage.py
    backend/settings.py
    backend/wsgi.py
    backend/asgi.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError: